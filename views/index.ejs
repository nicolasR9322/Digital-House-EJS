<!DOCTYPE html>
<html>
  <%- include("./partials/head")  %> 
  <body>
    <!-- Navbar (sit on top) -->
    <%- include("./partials/navbar") %> 

    <!-- Header -->
    <%- include("./partials/header") %> 

<div>
  <h1>PRUEBA</h1>

</div>


    <!-- Page content -->
    <div class="ps-content">
      <!-- About Section -->
      <div class="ps-row ps-padding-64" id="about">
        <div class="ps-col m6 ps-padding-large ps-hide-small">
          <img
            src="/images/principal.jpg"
            class="ps-round ps-image ps-opacity-min"
            alt="SAbores que enamoran"
            width="600"
            height="750"
          />
        </div>

        <%- include("./partials/about")  %> 
      </div>

      <hr />
      <!-- Menu Section -->
      <div class="ps-row ps-padding-64" id="menu">
        <div class="ps-col l6 ps-padding-large">
          <h1 class="ps-center">Nuestro Menu</h1>
          <br />
<% producto.forEach(plato => { %>
 <a href="/detalle/<%= plato.id %> "><h4><%= plato.nombre  %> </h4></a>
 <p class="ps-text-grey">
  <%= plato.descripcion %> 
 </p>
 <br />
 <% }) %>
</div>

<div class="ps-col l6 ps-padding-large ps-img-menu">
  <img
  src="/images/menu.jpg"
  class="ps-round ps-image ps-opacity-min"
  alt="Menu"
  />
</div>
      </div>


      
      <!-- Footer -->
      <%- include("./partials/footer") %> 
    </div>
  </body>
</html>
